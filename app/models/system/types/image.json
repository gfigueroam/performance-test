{
  "$id": "http://hmhco.com/image.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "description": {
      "$id": "/properties/description",
      "type": "string",
      "description": "A description of the image.",
      "examples": [
        "This shows how to perform three digit subtraction."
      ]
    },
    "format": {
      "$id": "/properties/format",
      "type": "string",
      "enum": ["jpg", "png", "gif"],
      "description": "The image format type",
      "examples": [
        "jpg"
      ]
    },
    "height": {
      "$id": "/properties/height",
      "type": "integer",
      "description": "The height of the image.",
      "examples": [
        768
      ]
    },
    "location": {
      "$id": "/properties/location",
      "type": "string",
      "description": "The location of the image.",
      "examples": [
        "http://hmhco.com/image.jpg"
      ]
    },
    "size": {
      "$id": "/properties/size",
      "type": "integer",
      "description": "Size in bytes of the image.",
      "examples": [
        786432
      ]
    },
    "width": {
      "$id": "/properties/width",
      "type": "integer",
      "description": "The width of the image.",
      "examples": [
        1024
      ]
    }
  },
  "required": [
    "description",
    "format",
    "height",
    "location",
    "size",
    "width"
  ],
  "additionalProperties": false
}
